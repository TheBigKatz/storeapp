<div>

<h1> My Anything Store! </h1>

<% @products.each do |product| %>
<h3><%= link_to product.name.to_s.upcase, "/products/#{product.id}" %></h3>
<h3><%= product.price %></h3>
<img src="<%= product.image %>"></h3>
<h3><%= product.description %></h3>
<h5><a data-confirm="Are You Sure?" data-method="delete"     href="/products/<%= product.id %>" rel="nofollow">Delete</a></h5>
<% end %>
</div>